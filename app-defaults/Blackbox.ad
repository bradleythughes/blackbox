! This file is just for defaults, when blackbox is exited or restarted, it will
! write the file ~/.blackboxrc, which will contain the defaults set in this
! file, this is only for the first time blackbox is run. DO NOT COPY THIS FILE
! to ~/.blackbox

session.colorsPerChannel:		4
session.toolbarRaised:			False
session.clockFormat:			12
session.imageDither:			True
session.workspaces:			3

!session.menuFile:			written on exit/restart
!session.styleFile:			written on exit/restart

