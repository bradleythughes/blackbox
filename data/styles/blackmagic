toolbar:			Raised Diagonal   Gradient Bevel1
toolbar.label:			Flat   Diagonal   Gradient
toolbar.clock:			Sunken Diagonal   Gradient Bevel1
toolbar.button:			Flat   Vertical   Gradient
toolbar.button.pressed:		Sunken Diagonal   Gradient Bevel1
toolbar*color:			rgb:2/2/4
toolbar*colorTo:		black
toolbar.clock.color:		black
toolbar.clock.colorTo:		rgb:2/2/4
toolbar.textColor:		lightgrey

window.focus:			Raised Diagonal   Gradient Bevel1
window.unfocus:			Raised Diagonal   Gradient Bevel1
window*button:			Raised Vertical   Gradient Bevel1
window*button.pressed:		Sunken Diagonal   Gradient Bevel1
window.handle:			Raised Diagonal   Gradient Bevel1
window.frame:			Raised Solid               Bevel1
window.frame.color:		rgb:2/2/4
window.focus*color:		rgb:4/4/2
window.focus*colorTo:		rgb:2/2/1
window.unfocus*color:		black
window.unfocus*colorTo:		rgb:2/2/4
window.button.pressed.color:	rgb:8/8/4
window.button.pressed.colorTo:	rgb:2/2/1
window.focus.textColor:		white
window.unfocus.textColor:	darkgrey

menu.title:			Flat   Vertical   Gradient
menu.frame:			Raised Diagonal   Gradient Bevel1

menu.title.color:		rgb:4/4/2
menu.title.colorTo:		rgb:2/2/1
menu.frame.color:		rgb:2/2/4
menu.frame.colorTo:		black
menu.frame.highlightColor:	rgb:4/4/2
menu.title.textColor:		white
menu.frame.textColor:		lightgrey
menu.frame.hiTextColor:		white

borderColor:			rgb:2/2/4
handleWidth:			8
bevelWidth:			3

titleJustify:			CenterJustify
menuJustify:			LeftJustify
moveStyle:			Opaque

titleFont:     -*-helvetica-medium-r-*-*-*-120-*-*-*-*-*-*
menuFont:      -*-helvetica-medium-r-*-*-*-120-*-*-*-*-*-*

rootCommand:			bsetroot -mod 4 4 -bg black -fg rgb:2/2/4

